package day1;

public class code5 {
		public static void main(String[] args) {
		int a=10;
		int b= --a + ++a + a++ + a-- + --a;
		System.out.println(a);
		System.out.println(b);
	    System.out.println("==========");
	    int p=10;
	    int q=12;
	    int max=p>q?p:q;
	    System.out.println(max);
   }
}
