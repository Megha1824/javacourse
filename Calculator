package lab3;

public class Calculator 
{
	int add(int x, int y)  
	{
		return x+y;
	}
	int add(int x,int y, int z)
	{
		return x+y+z;
	}
	double add(double x, double y )
	{
		return x+y;
	}

	public static void main(String[] args) 
	{
		Calculator a= new Calculator(); 
		System.out.println(a.add(24, 5));
		System.out.println(a.add(136,15, 25));
		System.out.println(a.add(25.6,58.7));
	}
}
